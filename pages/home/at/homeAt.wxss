/* pages/homeAt/homeAt.wxss */

.swiper-container{
  z-index:200;  
  position: absolute;
  width: 720rpx;
  height: 360rpx;
  margin-left: 15rpx;
}

.swiper-image{
    width: 720rpx;
    height: 400rpx;
    z-index:201;  
    position: absolute;

}

.swiper-info-title-container{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 200rpx;
  z-index:202;  
  position: absolute;
  color:whitesmoke;
}

.home-middle-container{
  background-color: white;
  border-top-right-radius: 15rpx;
  border-top-left-radius: 15rpx;
  margin-top: 400rpx;
}

.match-active-box{
  width: 100%;
  display: flex;
  margin-top: 20rpx;
  flex-direction: row;
  flex-wrap: wrap;
}

.match-active-box navigator{
  width: 150rpx;
  height: 200rpx;
  text-align: center;
  margin-left: 15rpx;
}


::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}